@model ModelProject.SLabelsModel
@{
    ViewBag.Title = "成品库管理";

    List<SelectListItem> Crr = new List<SelectListItem>();
    Crr.Add(new SelectListItem { Text = "全部", Value = "-1" });
    Crr.Add(new SelectListItem { Text = "未确认", Value = "0" });
    Crr.Add(new SelectListItem { Text = "已入库", Value = "1" });

    List<SelectListItem> PCrr = new List<SelectListItem>();
    PCrr.Add(new SelectListItem { Text = "全部", Value = "-1" });
    PCrr.Add(new SelectListItem { Text = "销售产品", Value = "1" });
    PCrr.Add(new SelectListItem { Text = "预投产品", Value = "2" });
    PCrr.Add(new SelectListItem { Text = "盘点产品", Value = "3" });

    List<SelectListItem> PSize = new List<SelectListItem>();
    PSize.Add(new SelectListItem { Text = "每页10", Value = "10" });
    PSize.Add(new SelectListItem { Text = "每页20", Value = "20" });
    PSize.Add(new SelectListItem { Text = "每页30", Value = "30" });
    PSize.Add(new SelectListItem { Text = "每页40", Value = "40" });
    PSize.Add(new SelectListItem { Text = "每页80", Value = "80" });
}

<p class="currpoint">
    <a href="@Url.Action("Index","Home")">首页</a>  -> 
    <a href="@Url.Action("Index","WIP")">生产管理</a>  -> 
    <a href="Javascript:void(0)" class="crrtab">成品库管理</a>
</p>

<div class="search">
    <ul class="searchtab"><li class="current">条件搜索</li></ul>
    <div style="clear:both"></div>
    <div class="searchbox clearfix">
        @using (Html.BeginForm("Index", "Labels", FormMethod.Post, new { @class = "form" }))
        {
            <div class="form_item">
			    <label class="label">所属系列：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.product_SN_id, Model.XLDroList, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
			    <label class="label">所属区域：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.product_area_id, Model.AreaDroList, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
                <label class="label">品名：</label>
                <div class="form-field">
                   @Html.TextBoxFor(k => k.productName,  new { @class="inputtext"})
                </div>
            </div>
            <div class="form_item">
			    <label class="label">所属仓库：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.inv_id, Model.CKDroList, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
			    <label class="label">所属材质：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.wood_id, Model.MCDroList, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
			    <label class="label">状态：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.status, Crr, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
			    <label class="label">所属方式：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.ProState, PCrr, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
			    <label class="label">入库时间：</label>
			    <div class="form-field">
                    @Html.TextBoxFor(k => k.StartTime, new { @class="sputtxt",@style="width:86px;",@ref="datepicker",@placeholder="开始时间"}) -
                    @Html.TextBoxFor(k => k.EndTime, new { @class="sputtxt",@style="width:86px;",@ref="datepicker",@placeholder="结束时间"})
			    </div>
		    </div>
            <div class="form_item">
			    <label class="label">每页显示：</label>
			    <div class="form-field">
                   @Html.DropDownListFor(k => k.PagePSize, PSize, new { @class="drotext"})
			    </div>
		    </div>
            <div class="form_item">
                <label class="label">合同编号：</label>
                <div class="form-field">
                   @Html.TextBoxFor(k => k.ProSN,  new { @class="inputtext"})
                </div>
            </div>
            <div class="form_item">
                <label class="label">订货人：</label>
                <div class="form-field">
                   @Html.TextBoxFor(k => k.SaleName,  new { @class="inputtext"})
                </div>
            </div>
            <div class="clear"></div>
            <div class="form-item-submit">
                <input type="submit" value="提交" class="inputsubbtn fr mb20 mr20" style="width:100px;" />
            </div>
        }
    </div>
</div><!--search end -->

<div class="mod">
    <div class="hd">
        <h2>搜索结果</h2>
        <ul class="filter">
            <li><a class="selectAll btn bg-orange" href="javascript:void(0)"><span class="wh">全选</span></a></li>
            <li><a class="unSelect btn bg-orange" href="javascript:void(0)"><span class="wh">全不选</span></a></li>
            <li class="toExcelCWout"><a class="btn bg-red" href="javascript:void(0)"><span class="wh">财务成品库存</span></a></li>
            <li class="toExcelout"><a class="btn bg-red" href="javascript:void(0)"><span class="wh">导出成品库存</span></a></li>
            <li><a class="delSelect btn bg-qblue" href="javascript:void(0)" url="@Url.Action("DeliveryMore","Labels",new { DeliveryTime=DateTime.Now.ToString("yyyy-MM-dd")})" title="选中送货维修"><span class="wh">选中送货维修</span></a></li>
            <li class="adddialog" url="@Url.Action("Add", "Labels")" title="盘点入库" ref="0"><a class="btn bg-green" href="javascript:void(0)"><span class="wh">盘点入库</span></a></li>
            <li><a class="TransferSelect btn bg-blue" href="javascript:void(0)" url="@Url.Action("CheckMore","Labels")"  title="选中入库"><span class="wh">选中入库</span></a></li>
            <li><a class="delSelect btn bg-qblue" href="javascript:void(0)" url="@Url.Action("Work","Labels")" ><span class="wh">选中生产</span></a></li>
            <li><a class="TransferSelect btn bg-qblue" href="javascript:void(0)" url="@Url.Action("TransferMore","Labels")" title="选中移库"><span class="wh">选中移库</span></a></li>
        </ul>
    </div>
</div>

<div id="ResultList" class="xTable">
   @Html.Action("PageList", "Labels", new { SRmodels=Model})
</div>


<script type="text/javascript">
    $(function () {
        $(".toExcelout").click(function () {
            var product_SN_id = $("#product_SN_id").val();
            var product_area_id = $("#product_area_id").val();
            var productName = $("#productName").val();
            var inv_id = $("#inv_id").val();
            var status = $("#status").val();
            var ProState = $("#ProState").val();
            var StartTime = $("#StartTime").val();
            var EndTime = $("#EndTime").val();
            var ExcelUrl = '@Url.Action("ToExcelOut","Labels")';
            window.location.href = "/Labels/ToExcelOut?product_SN_id=" + product_SN_id + '&product_area_id=' + product_area_id + '&productName=' + productName + '&inv_id=' + inv_id + '&status=' + status + '&ProState=' + ProState + '&StartTime=' + StartTime + '&EndTime=' + EndTime;
            //$.post(ExcelUrl, {}, function (d) { });
        });
        $(".toExcelCWout").click(function () {
            var product_SN_id = $("#product_SN_id").val();
            var product_area_id = $("#product_area_id").val();
            var productName = $("#productName").val();
            var inv_id = $("#inv_id").val();
            var status = $("#status").val();
            var ProState = $("#ProState").val();
            var StartTime = $("#StartTime").val();
            var EndTime = $("#EndTime").val();
            var ExcelUrl = '@Url.Action("ToExcelOut","Labels")';
            window.location.href = "/Labels/ToExcelCWOut?product_SN_id=" + product_SN_id + '&product_area_id=' + product_area_id + '&productName=' + productName + '&inv_id=' + inv_id + '&status=' + status + '&ProState=' + ProState + '&StartTime=' + StartTime + '&EndTime=' + EndTime;
            //$.post(ExcelUrl, {}, function (d) { });
        });
    });
</script>